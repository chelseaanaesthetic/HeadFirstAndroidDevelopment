<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hfad.vegeasy2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".NutritionalConcernsCategoryActivity"
            android:label="@string/nutritional_concerns"
            android:parentActivityName=".HealthCategoryActivity">
        </activity>

        <activity
            android:name=".NutritionalConcernsActivity"
            android:label="@string/nutritional_concerns"
            android:parentActivityName=".NutritionalConcernsCategoryActivity">
        </activity>

        <activity
            android:name=".HealthCategoryActivity"
            android:label="@string/health"
            android:parentActivityName=".MainActivity">
        </activity>
    </application>

</manifest>
